lock\"\u003E\r\n            \u003Cdiv class=\"popup__close\"\u003E\u003Ca href=\"#\" onclick=\"$('#popup-subscribe').fadeOut(); return false;\" class=\"popup__close-btn\"\u003E\u003C\u002Fa\u003E\u003C\u002Fdiv\u003E\r\n            \u003Cdiv class=\"form form-ask-for-help\"\u003E\r\n                \u003Cform class=\"form\" method=\"post\" action=\"\" onsubmit=\"return false;\"\u003E\r\n                    \u003Cdiv class=\"form__title\"\u003EЗапишитесь на бесплатную консультацию:\u003C\u002Fdiv\u003E\r\n                    \u003Cdiv style=\"clear: both; border-top: 1px solid #ddd;\"\u003E \u003C\u002Fdiv\u003E\r\n                    \u003Cdiv class=\"form__content\"\u003E\r\n                        \u003Clabel class=\"label form__label\"\u003E\r\n                            \u003Cdiv\u003E+7 (495) 777 28 20\u003C\u002Fdiv\u003E\r\n                        \u003C\u002Flabel\u003E\r\n                        \u003Clabel class=\"label form__label\"\u003E\r\n                            \u003Cdiv\u003E\u003Ca href=\"mailto:office@bgplaw.com\" class=\"popup-link\"\u003Eoffice@bgplaw.com\u003C\u002Fa\u003E\u003C\u002Fdiv\u003E\r\n                        \u003C\u002Flabel\u003E\r\n                    \u003C\u002Fdiv\u003E\r\n                \u003C\u002Fform\u003E\r\n            \u003C\u002Fdiv\u003E\r\n            \u003Cdiv class=\"form form-subscribe\"\u003E\r\n                \u003Cform class=\"form\" method=\"post\" action=\"\" onsubmit=\"sendFormSubscribe(this); return false;\"\u003E\r\n                    \u003Cdiv class=\"form__title\"\u003EПодписаться на рассылку\u003C\u002Fdiv\u003E\r\n                    \u003Cdiv style=\"clear: both; border-top: 1px solid #ddd;\"\u003E \u003C\u002Fdiv\u003E\r\n                    \u003Cdiv class=\"form__responce\"\u003E\u003C\u002Fdiv\u003E\r\n                    \u003Cdiv class=\"form__content\"\u003E\r\n                        \u003Clabel class=\"label form__label\"\u003E\r\n                            \u003Cdiv class=\"label__title\"\u003EТема рассылки\u003C\u002Fdiv\u003E\r\n                            \u003Cselect name=\"theme\" type=\"text\" class=\"label__item form__input\" style=\"padding-top: 25px;\"\u002F\u003E\r\n                            \u003Coption\u003EM&A и реструктуризация бизнеса\u003C\u002Foption\u003E\r\n                            \u003Coption\u003EАдвокатские расследования\u003C\u002Foption\u003E\r\n                            \u003Coption\u003EАнтимонопольное\u002FКонкурентное право\u003C\u002Foption\u003E\r\n                            \u003Coption\u003EБанкротство\u003C\u002Foption\u003E\r\n                            \u003Coption\u003EЗдравоохранение и технологии\u003C\u002Foption\u003E\r\n                            \u003Coption\u003EМеждународное налоговое планирование\u003C\u002Foption\u003E\r\n                            \u003Coption\u003EМеждународный коммерческий арбитраж\u003C\u002Foption\u003E\r\n                            \u003Coption\u003EНалоговые споры\u003C\u002Foption\u003E\r\n                            \u003Coption\u003EРазрешение споров\u003C\u002Foption\u003E\r\n                            \u003Coption\u003EСемейное право\u003C\u002Foption\u003E\r\n                            \u003Coption\u003EУголовно-правовая защита бизнеса\u003C\u002Foption\u003E\r\n                            \u003C\u002Fselect\u003E\r\n                        \u003C\u002Flabel\u003E\r\n                        \u003Clabel class=\"label form__label\"\u003E\u003Cinput name=\"first_name\" type=\"text\" required=\"required\" class=\"label__item form__input\"\u003E\r\n                            \u003Cdiv class=\"label__title\"\u003EИмя\u003C\u002Fdiv\u003E\r\n                        \u003C\u002Flabel\u003E\r\n                        \u003Clabel class=\"label form__label\"\u003E\u003Cinput name=\"last_name\" type=\"text\" required=\"required\" class=\"label__item form__input\"\u003E\r\n                            \u003Cdiv class=\"label__title\"\u003EФамилия\u003C\u002Fdiv\u003E\r\n                        \u003C\u002Flabel\u003E\r\n                        \u003Clabel class=\"label form__label\"\u003E\u003Cinput name=\"company_name\" type=\"text\" required=\"required\" class=\"label__item form__input\"\u003E\r\n                            \u003Cdiv class=\"label__title\"\u003EКомпания\u003C\u002Fdiv\u003E\r\n                        \u003C\u002Flabel\u003E\r\n                        \u003Clabel class=\"label form__label\"\u003E\u003Cinput name=\"job\" required=\"required\" class=\"label__item form__input\"\u003E\r\n                            \u003Cdiv class=\"label__title\"\u003EДолжность\u003C\u002Fdiv\u003E\r\n                        \u003C\u002Flabel\u003E\r\n                        \u003Clabel class=\"label form__label\"\u003E\u003Cinput name=\"email\" autocomplete=\"on\" type=\"email\" required=\"required\"\r\n                                                                class=\"label__item form__input form__input--email\"\u003E\r\n                            \u003Cdiv class=\"label__title\"\u003EE-mail\u003C\u002Fdiv\u003E\r\n                        \u003C\u002Flabel\u003E\r\n                        \u003Clabel class=\"switch clearfix\" style=\"margin: 0 25px 70px 0;\"\u003E\r\n                            \u003Cinput type=\"checkbox\" name=\"approve\" class=\"success\"\u002F\u003E\r\n                            \u003Cspan class=\"checkbox-slider round\"\u003E\u003C\u002Fspan\u003E\r\n                        \u003C\u002Flabel\u003E\r\n                        \u003Cdiv class=\"form__policy\"\u003EОтправляя форму, я подтверждаю согласие на обработку и хранение персональных данных.\u003Cbr\u002F\u003E— \u003Ca\r\n                                    href=\"\u002Fprivacy-policy\u002F\" style=\"color: #000;\" target=\"blank\"\u003EПолитика конфиденциальности\u003C\u002Fa\u003E\u003Cbr\u002F\u003E— \u003Ca\r\n                                    href=\"\u002Fconsent-to-the-processing-of-personal-data\u002F\" style=\"color: #000;\" target=\"blank\"\u003EСогласие на обработку персональных\r\n                                данных\u003C\u002Fa\u003E\u003C\u002Fdiv\u003E\r\n                        \u003Cbr\u002F\u003E\r\n                        \u003Cinput type=\"hidden\" name=\"type_key\" value=\"news\" \u002F\u003E\r\n                        \u003Cbutton type=\"submit\" class=\"btn\"\u003EОтправить\u003C\u002Fbutton\u003E\r\n                    \u003C\u002Fdiv\u003E\r\n                \u003C\u002Fform\u003E\r\n            \u003C\u002Fdiv\u003E\r\n        \u003C\u002Fdiv\u003E\r\n    \u003C\u002Fdiv\u003E\r\n\u003C\u002Fdiv\u003E\r\n\u003Cscript type=\"text\u002Fjavascript\"\u003E\r\n    function sendFormSubscribe(_form) {\r\n        $(_form).find('button[type=\"submit\"]').attr('disabled', true).html('\u003Cdiv class=\"loader\"\u003E\u003C\u002Fdiv\u003E');\r\n        event.preventDefault();\r\n        var data = $(_form).serialize();\r\n        $.ajax({\r\n            url: 'https:\u002F\u002Fadmin.bgplaw.com\u002Fsubscribe\u002F',\r\n            type: 'POST',\r\n            cache: false,\r\n            data: {\r\n                'data': data,\r\n                'confirm': 'true'\r\n            },\r\n            dataType: 'html',\r\n            success: function (html) {\r\n                $(_form).find('.form__responce').html(html);\r\n                $(_form).find('button[type=\"submit\"]').attr('disabled', false).html('Отправить');\r\n            }\r\n        });\r\n    }\r\n    function showFormSubscribe() {\r\n        $('#popup-subscribe div.form').hide();\r\n        $('#popup-subscribe div.form-subscribe').show();\r\n        $('#popup-subscribe').fadeIn();\r\n    }\r\n    function showFormAskForHelp() {\r\n        $('#popup-subscribe div.form').hide();\r\n        $('#popup-subscribe div.form-ask-for-help').show();\r\n        $('#popup-subscribe').fadeIn();\r\n    }\r\n    function tryVideo(v) {\r\n        $.ajax({\r\n            url: 'https:\u002F\u002Fadmin.bgplaw.com\u002Ftry-video\u002F',\r\n            type: 'POST',\r\n            cache: false,\r\n            data: { 'v': v, 'c': getCookie('bgpvct') },\r\n            dataType: 'html',\r\n            success: function (html) {\r\n                $('.page-videos-responce').html(html);\r\n            }\r\n        });\r\n    }\r\n    function runVideo(url) {\r\n        $.fancybox.close();\r\n        $.fancybox.open({src: url});\r\n        $('.page-videos-responce').html('');\r\n    }\r\nfunction getCookie(name) {\r\n  let matches = document.cookie.match(new RegExp(\r\n    \"(?:^|; )\" + name.replace(\u002F([\\.$?*|{}\\(\\)\\[\\]\\\\\\\u002F\\+^])\u002Fg, '\\\\$1') + \"=([^;]*)\"\r\n  ));\r\n  return matches ? decodeURIComponent(matches[1]) : undefined;\r\n}\r\n\u003C\u002Fscript\u003E\r\n\u003Cstyle type=\"text\u002Fcss\"\u003E\r\n.popup-link, .popup-link:hover{\r\ncolor: #a6001e;\r\n}\r\n\u003C\u002Fstyle\u003E\r\n\u003Clink type=\"text\u002Fcss\" rel=\"stylesheet\" href=\"\u002Fswiper\u002Fswiper-bundle.min.css\"\u002F\u003E\r\n\u003Cscript type=\"text\u002Fjavascript\" src=\"\u002Fswiper\u002Fswiper-bundle.min.js\"\u003E\u003C\u002Fscript\u003E\r\n\r\n\u003C!-- Yandex.Metrika counter --\u003E\r\n\u003Cscript type=\"text\u002Fjavascript\" \u003E\r\n   (function(m,e,t,r,i,k,a){m[i]=m[i]||function(){(m[i].a=m[i].a||[]).push(arguments)};\r\n   m[i].l=1*new Date();k=e.createElement(t),a=e.getElementsByTagName(t)[0],k.async=1,k.src=r,a.parentNode.insertBefore(k,a)})\r\n   (window, document, \"script\", \"https:\u002F\u002Fmc.yandex.ru\u002Fmetrika\u002Ftag.js\", \"ym\");\r\n\r\n   ym(48956744, \"init\", {\r\n        clickmap:true,\r\n        trackLinks:true,\r\n        accurateTrackBounce:true,\r\n        webvisor:true,\r\n        ecommerce:\"dataLayer\"\r\n   });\r\n\u003C\u002Fscript\u003E\r\n\u003Cnoscript\u003E\u003Cdiv\u003E\u003Cimg src=\"https:\u002F\u002Fmc.yandex.ru\u002Fwatch\u002F48956744\" style=\"position:absolute; left:-9999px;\" alt=\"\" \u002F\u003E\u003C\u002Fdiv\u003E\u003C\u002Fnoscript\u003E\r\