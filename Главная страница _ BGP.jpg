<script type="text/javascript">
    function sendFormSubscribe(_form) {
        $(_form).find('button[type="submit"]').attr('disabled', true).html('<div class="loader"></div>');
        event.preventDefault();
        var data = $(_form).serialize();
        $.ajax({
            url: 'https://admin.bgplaw.com/subscribe/',
            type: 'POST',
            cache: false,
            data: {
                'data': data,
                'confirm': 'true'
            },
            dataType: 'html',
            success: function (html) {
                $(_form).find('.form__responce').html(html);
                $(_form).find('button[type="submit"]').attr('disabled', false).html('Отправить');
            }
        });
    }
    function showFormSubscribe() {
        $('#popup-subscribe div.form').hide();
